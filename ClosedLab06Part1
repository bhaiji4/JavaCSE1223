
import java.util.Scanner;

public class ClosedLab06Part1 {

	public static void main(String[] args) {

		Scanner scanner = new Scanner (System.in);

				int j=1;
				while (j<=10){
					int i=1;
					while (i<=10) {
						System.out.printf("%4s", (i*j) );
						i++;
					}
					System.out.println();
					j++;
				
				}
	}

}

	

