
import java.util.Scanner;

public class ClosedLab05Part3 {

	public static void main(String[] args) {
		Scanner in=new Scanner (System.in);
		
		System.out.println("Enter a string to reverse: ");
		String input=in.nextLine();
		
		int k=input.length()-1; //last char of string number
		int j=input.charAt(k); //last actual char of the string
		
		
		//for (int i=0;i<=input.length()-1;i++){ //i going forward
			for (k=input.length()-1;k>=0;k--){
				System.out.print(input.charAt(k));
			}
		//}

	}

}
